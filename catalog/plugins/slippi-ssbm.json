{
  "id": "slippi-ssbm",
  "name": "SSBM Plugin",
  "description": "Reads live Slippi events from Dolphin.",
  "repository": "https://github.com/fkribs/SaltshakerSSBMPlugin",
  "commit": "cc6b93abf0c482595e4f9e6e90ea02a677fa2e1e",
  "version": "1.0.5",
  "artifactUrl": "https://github.com/fkribs/SaltshakerPlugins/releases/download/slippi-ssbm-v1.0.5/saltshaker-plugin-slippi-ssbm-1.0.5.tgz",
  "sha256": "d9ed689f3f99e1b6105df6dad14874963eff6d5f5bf03f74410b8fed47e3031a",
  "minClientVersion": "0.2.10",
  "permissions": [
    "file.read",
    "dolphin.subscribe"
  ],
  "resources": [
    {
      "id": "slippiUserFile",
      "path": "{home}/AppData/Roaming/Slippi Launcher/netplay/User/Slippi/user.json",
      "type": "json"
    }
  ],
  "settings": [
    {
      "key": "hangupOnGameEnd",
      "type": "boolean",
      "label": "Hang up on game end",
      "description": "If enabled, Saltshaker will disconnect the call when Dolphin emits GameEnd.",
      "default": true
    }
  ]
}
