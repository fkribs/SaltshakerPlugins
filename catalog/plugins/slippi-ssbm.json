{
  "id": "slippi-ssbm",
  "name": "SSBM Plugin",
  "description": "Reads live Slippi events from Dolphin.",
  "donationUrl": "https://buymeacoffee.com/fkribs",
  "repository": "https://github.com/fkribs/SaltshakerSSBMPlugin",
  "commit": "d52f9c905368bfbc9428a11cfac82131fe6ce486",
  "version": "1.0.6",
  "artifactUrl": "https://github.com/fkribs/SaltshakerPlugins/releases/download/slippi-ssbm-v1.0.6/saltshaker-plugin-slippi-ssbm-1.0.6.tgz",
  "sha256": "914abecfb3a78b4049164dd407a1300c1e430a88f7a41edb3d9529cd416e559a",
  "minClientVersion": "0.2.10",
  "permissions": [
    "file.read",
    "dolphin.subscribe"
  ],
  "resources": [
    {
      "id": "slippiUserFile",
      "path": "{home}/AppData/Roaming/Slippi Launcher/netplay/User/Slippi/user.json",
      "type": "json"
    }
  ],
  "settings": [
    {
      "key": "hangupOnGameEnd",
      "type": "boolean",
      "label": "Hang up on game end",
      "description": "If enabled, Saltshaker will disconnect the call when Dolphin emits GameEnd.",
      "default": true
    }
  ]
}
