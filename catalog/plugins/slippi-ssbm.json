{
  "id": "slippi-ssbm",
  "name": "SSBM Plugin",
  "description": "Reads live Slippi events from Dolphin.",
  "repository": "https://github.com/fkribs/SaltshakerSSBMPlugin",
  "commit": "6b2fa14f3a4023776a33cf9b46ca86a0ba2eb2a6",
  "version": "1.0.4",
  "artifactUrl": "https://github.com/fkribs/SaltshakerPlugins/releases/download/slippi-ssbm-v1.0.4/saltshaker-plugin-slippi-ssbm-1.0.4.tgz",
  "sha256": "fe45a4e3b346583b5b05ff9398a20ac1d756cde11ab13288c2f2beba8dd0aaf6",
  "minClientVersion": "0.2.10",
  "permissions": [
    "file.read",
    "dolphin.subscribe"
  ],
  "resources": [
    {
      "id": "slippiUserFile",
      "path": "{home}/AppData/Roaming/Slippi Launcher/netplay/User/Slippi/user.json",
      "type": "json"
    }
  ],
  "settings": [
    {
      "key": "hangupOnGameEnd",
      "type": "boolean",
      "label": "Hang up on game end",
      "description": "If enabled, Saltshaker will disconnect the call when Dolphin emits GameEnd.",
      "default": true
    }
  ]
}
